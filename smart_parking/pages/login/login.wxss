page{
  background-image: url('https://ss1.bdstatic.com/70cFvXSh_Q1YnxGkpoWK1HF6hhy/it/u=1673150750,3342013365&fm=26&gp=0.jpg');
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  width: 100%;
} 

.wrapper{
  position:relative;
  z-index: 90;
  padding-bottom: 50rpx;
}

.left-top-sign{
  font-size: 120rpx;
  color: #ffcc66;
  opacity: 0.5;
  position:relative;
  left: -16rpx;
}

.welcome{
  position:relative;
  left: 50rpx;
  top: -90rpx;
  font-size: 46rpx;
  color: #555;
}


.input-content{
  margin-top: 40rpx;
  width: 100%;
}


.input-item{
  display: flex;
  background-color: #FFFFFF;
  margin: 50rpx 0;
  width: 100%;
}

.input-item:last-child{
  margin-bottom: 0;
}

.input-item .tit{
   /* padding: 0 10rpx; */
   margin: 25rpx 10rpx;
   padding-top: 2rpx;
   display: inline-block;
   width: 8%;
   padding: 8rpx 0;
}

.input-item .text{
  font-size: 32rpx;
}

.input-item input{
  height: 60rpx;
  font-size: 30rpx;
  margin: 22rpx 0;
  color: #303133;
}
.confirm-btn{
  width: 630rpx!important;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 50rpx;
  margin-top: 70rpx;
  background: #d43c33;
  color: #fff;
  font-size: 32rpx;
  padding: 0;
}

.wx-login{
  width: 100%;
  margin: 40rpx auto !important;
  display: flex;
  justify-content: center;
}

.wx-login button{
  border-radius: 40rpx;
}

.confirm-btn2:after{
  border-radius: 100px;
}

.forget-section{
  font-size: 28rpx;
  color: #4399fc;
  text-align: center;
  margin-top: 40rpx;
}

.register-section{
  position:absolute;
  left: 0;
  bottom: 80rpx;
  width: 100%;
  font-size: 28rpx;
  color: #606266;
  text-align: center;

}
.register-section text{
  color: #4399fc;
  margin-left: 10rpx;
}

