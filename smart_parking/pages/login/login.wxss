.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconfont{
  font-size: 40rpx;
}

.icon-ren:before {
  content: "\e60e";
}

.icon-mima:before {
  content: "\e609";
}
