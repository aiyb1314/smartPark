page{
  height: 100%;
  width: 100%;
  background-color: #f6f6f6;
}

#orderContainer{
  display: flex;
  flex-flow: column;
  height: 100%;
}

#orderContainer .header{
  width: 95%;
  margin: 20rpx auto;
  background-color: white;
  height: 8%;
  display: flex;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx
}

.header text{
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    margin: auto 20rpx;
    font-size: 40rpx;
    background-color: #ccffff;
    text-align: center;
    border-radius: 50rpx;
    line-height: 50rpx;
}

.header .header-view{
  width: 80%;
  font-size: 10rpx;
  font-weight: bold;
  display: flex;
  flex-direction: column;
 
}

.header-view text{
  font-size: 30rpx;
  text-align: left;
  width: 100%;
  background-color: #fff;
  line-height: 20rpx;
}

.header-view-one{
  padding-top: 35rpx;
}

.header-view-two{
  color: gray;
  font-size: 20rpx !important;
  padding-bottom: 15rpx;
}

.middle{
  width: 100%;
  height: 30%;
  background-color: white;
  padding: 20rpx;
}

.middle-title .tag{
  color: gray;
  font-size: 25rpx;
}

.middle-title .locate{
  margin: 0 14rpx;
  font-weight: bold;
}

.middle-content{
  display: flex;
  margin-top: 30rpx;
  width: 100%;
  height: 50%;
}

.middle-content .img{
  width: 25%;
  height: 90%;
  border-radius: 15rpx;
  margin-right: 30rpx;
}

.middle-content .title{
  font-size: 25rpx;
  display: flex;
  flex-direction: column;
  height: 80%;
}

.middle-content .title text:first-child{
  margin-top: 0;
}

.middle-content .title text{
  display: inline-block;
  margin-top: 20rpx;
}

.middle-footer{
  margin-top: 15rpx;
  position: relative;
  height: 25%;
}

.middle-footer .btn{
  border: 1rpx gray solid;
  font-size: 20rpx;
  border-radius: 20rpx;
  position: absolute;
  right: 20rpx;
}

.middle-footer .price{
  position: absolute;
  font-size: 25rpx;
  right: 30rpx;
  bottom: 0rpx;
}

.middle-footer .price text:first-child{
  font-weight: bold;
}

.iconfl-renminbi{
  font-size: 20rpx;
  color: #FC5531;
  padding-top: 10rpx;
  display: inline-block;
  margin-left: 5rpx;
}

.middle-footer text:last-child{
  font-size: 30rpx;
  color: #FC5531;
}

.footer{
  background-color: #fff;
  margin-top:20rpx;
  padding-left: 25rpx;
  display: flex;
  flex-direction: column;
}

.footer-title{
  height: 60rpx;
  line-height: 60rpx;
}

.footer .footer-title text:last-child{
  float: right;
  font-size: 23rpx;
  color: gray;
}

.footer-title .iconxiangxia{
  padding-right: 30rpx;
  margin-left: 5rpx;
}

.footer-content{
  width: 100%;
}

.content-item{
  margin-top: 20rpx;
  font-size: 23rpx;
  width: 100%;
}


.content-item text:first-child{
  display: inline-block;
  width: 30%;
}

.content-detail text:first-child{
    display: inline-block;
    vertical-align: middle;
    margin-bottom: 50rpx;
}

.content-exe{
  margin-top: -5rpx;
}

.content-item text:last-child{
  margin-top: 5rpx;
  display: inline-block;
  width: 67%;
}

.footer-btn{
    margin-top: 20rpx;
    /* padding: 20rpx; */
    text-align: center;
    border-top: 1rpx solid gray;
    margin-right: 30rpx;
    box-sizing: border-box;
    line-height: 80rpx;
}

.iconbiaoqiankuozhan_lianxiren-374{
  color: skyblue;
  font-size: 30rpx;
  line-height: 80rpx;
}

.footer-btn .btn{
  display: inline;
  border: 1px white;
  font-size: 25rpx;
  margin: auto 0;
}