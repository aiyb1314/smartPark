  content: "\ef89";
}

Page{
  background-color:#F5F5F5;
  height: 100%;
}

.orderContainer{
  width: 100%;
  height: 100%;
}

.header{
  margin: 20rpx auto;
  width: 96%;
  background-color: white;
  border-radius: 20rpx;
  height: 15%;
  display: flex;
}

.header image{
  width:70rpx;
  height:70rpx;
  margin: auto 20rpx;
}

.right{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 28rpx;
}

.right text:last-child{
  color: red;
  font-size: 24rpx;
  margin-bottom: 10px;
}

.middle{
  width: 96%;
  height: 50%;
  background-color: white;
  margin: 0 auto;
  border-radius: 30rpx;
}

.middle::before{
  content: '';
  display: table;
}

.middle .middle-header{
  margin: 20rpx;
  line-height: 60rpx;
}

.middle-header text:last-child{
  vertical-align: top;
}

.middle .middle-header text:first-child{
  font-size: 50rpx;
  color: skyblue;
  margin-right: 18rpx;
}

.middle-title{
  margin: 20rpx;
  display: flex;
}

.title-right{
  display: flex;
  flex-direction: column;
  width: 70%;
  margin-left: 20rpx;
}

.title-right text{
  font-size: 28rpx;
  word-break: break-all;
  margin-top: 10rpx;
}

.title-right text:first-child{
  margin-top: 0;
  color: orange;
  font-size: 37rpx;
}

.middle-content{
  font-size: 28rpx;
  margin-top: 30rpx;
  margin-left: 100rpx;
}

.middle-content .content-item{
  margin-top: 40rpx;
}

.content-item .bzText{
  line-height: 50rpx;
}

.content-item .bzInput{
  vertical-align: top;
  display: inline-block;

}

.middle-content .content-item text:first-child{
  margin-right: 40rpx;
}

.icon-RectangleCopy text:first-child{
  font-size: 28rpx;
}

.totalprice{
  float: right;
  /* margin-right: ; */
  margin: 20rpx;
}

.totalprice text:first-child{
  margin-right: 10rpx;
  color: #3C3C3C;
  font-size: 24rpx;
  opacity: 0.7;
}

.totalprice text:last-child{
  font-size: 26rpx;
  color: red;
  opacity: 0.7;
}

.payOption{
  padding: 1rpx;
  background-color: #fff;
  width: 96%;
  height: 10%;
  margin: 20rpx auto;
  border-radius: 30rpx;
}


.payOption .payItem{
  margin: 20rpx;

}

.payItem text{

  font-size: 40rpx;
  vertical-align: middle;
}

.payItem text:nth-child(2){
  font-size: 28rpx;
}

.payItem text:last-child{
  font-size: 28rpx;
}

.itemtag{
  float: right;
}

.payItem .noSerct{
  font-size: 30rpx;
}

.payItem text:first-child{
  margin-right: 13rpx;
}

.word-right{
  float: right;
  font-size: 30rpx!important;
  padding-top: 4rpx;
}

.icon-zhifu{
  color: skyblue;
}

.niName{
  margin-left: 40rpx;
}

.niName text:last-child{
  font-size: 26rpx;
  color: #3C3C3C;
}

.icon-xianshi_xuanze{
  color: orange !important;
  opacity: 0.6;
  font-size: 32rpx !important;
}

.footer{
  width: 100%;
  position: fixed;
  height: 15%;
  bottom: 0;
  background-color: #fff;
}

.footer-head{
  margin-top: 15rpx;
  margin-left: 20rpx;
}

.icon-anquan{
  color: #42A5F5;
  font-size: 60rpx;
  vertical-align: middle;
}

.footer .footer-head text:last-child{
  font-size: 28rpx;
  color: red;
  display: inline-block;
  margin-left: 13rpx;
}

.footer-final{
  margin: 20rpx;
  float: right;
}

.footer-final text:first-child{
    font-size:25rpx;
    color: #d5d5d5;
    margin-right: 15rpx;
}

.footer-final text:last-child{
  display: inline-block;
  border-radius: 30rpx;
  padding: 14rpx;
  font-size: 28rpx;
  color: white;
  background-color: orange;
}

.cancelOrder{
  font-size: 21rpx;
  color: red;
  opacity: 0.5;
}

.hiden{
  display: none;
}