.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jiage:before {
  content: "\e600";
}

.icon-jiage{
  color: red;
  font-size: 20rpx;
  opacity: 0.8;
}

.icon-jiage text:first-child{
  font-size: 28rpx;
}

page{
  background-color:#F6F6F6
}

.thisContianer{
  height: 100%;
  width: 100%;
}

.headerTitle{
  background-color: #fff;
  border-radius: 0 0 20rpx 20rpx;
}


.headerTitle text{
  display: inline-block;
  line-height: 90rpx;
  width: 46%;
  text-align: center;
  color: white; 
  margin: 20rpx;
}

.headerTitle text:first-child{
  background-color: #FBE09F;
  border-radius: 20rpx 10rpx 10rpx 20rpx;
}

.headerTitle text:last-child{
  background-color: #F8C2B3;
  border-radius:  10rpx 20rpx 20rpx 10rpx;
  margin-left: -10rpx;
}

.parkContainer{
  border-radius: 20rpx;
  height: 100%;
  width: 100%;
  margin: 20rpx auto;
  background-color: #fff;
}

.all{
  padding: 16rpx;
  width: 96%;
  margin: 0 auto;
  border-bottom: 2rpx solid #EDEEEF;
  display: flex;
  background-color: #fff;
}

.all .left{
  width: 26%;
  height: 95%;
  margin: auto 0;
  border-radius: 15rpx;
}

.all .content{
  letter-spacing: 4rpx;
  width: 50%;
  margin: 0 30rpx;
  display: flex;
  flex-flow: column;
  justify-content: space-around;
}

.all .content .title{
  font-weight: bold;
  font-size: 30rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.hideView{
  position: relative;
}

.hideInfo{
  background-image: url('https://img.52z.com/upload/news/image/20200317/20200317075838_48364.jpg');
}

.hideText{
  width: 100%;
  font-size: 60rpx;
  color: aquamarine;
  font-family: 'cursive';
  /* padding: 400rpx 95rpx; */
  text-align: center;
  padding: 400rpx 0;
}