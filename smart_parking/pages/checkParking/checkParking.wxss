page{
  background-color:#F6F6F6;
  height: 100%;
  font-size: 1.3rem
}

.checkContainer{
  height: 100%;
}

.checkContainer .content{
  height: 70%;
  display: flex;
  flex-flow: column;
  align-items: center;
}

.content .title{
  background-color: white;
  font-weight: bold;
  width: 95%;
  height: 20%;
  text-align: center;
  /* line-height: 200rpx; */
  font-size: 1.3rem;
  border-radius: 30rpx;
  margin-top: 10rpx;
}

.content .price{
  position: relative;
  text-align: left;
  padding: 30rpx 20rpx;
  height: 15%;
}

.content .stopTimes{
  position: absolute;
  right: 20rpx;
  font-size: 0.7rem;
  bottom: 30rpx;
  color: gray;
  
}

.content .info{
  text-align: left;
  padding: 20rpx;
  height: 10%;
  /* box-sizing: content-box; */
}

.info .infoTitle{
  border-right: black solid 1rpx;
  font-size: 30rpx;
  width: 45%;
  display: inline-block;
}

.orderTimeContainer{
  height: 28%;
}

.content .orderTime{ 
    /* padding: 20rpx; */
    height: 70%;
    white-space: nowrap;
    border: 1px solid gray;
    box-sizing: border-box
}

#ordertime{
  display: inline-block;
  margin-bottom: 10rpx;
}

.itemOrder{
    position: relative;
    margin: auto;
    width: 20%;
    height:100%;
    font-size: 25rpx;
    overflow: hidden;
    display: inline-block;
    border-right: gray 1rpx solid;
}

.itemOrder .lst{
  display: block;
  margin: 30rpx auto;
}

.content .location{
    position: relative;
    text-align: left;
    height: 20%;
    display: flex;
   
}

.location .left{
  width: 25%;
  height: 90%;
  border-radius: 20rpx;
  margin: auto 8rpx;
  /* margin: 20rpx; */
  /* box-sizing: border-box; */
}

.location .orderContainer{
  line-height: 50rpx;
  display: flex;
  flex-flow: column;
  width: 60%;
  height: 100%;
  justify-content: space-around;
  margin: 0 auto;
}

.orderContainer .carport{
  margin-top: 20rpx;
  color: #AEEEEE;
  font-size: 1.3rem;
}

.orderContainer view{
  font-size: 0.9rem;
}

.icon-yuyue1{
  font-size: 2rem!important;
  color: red;
  opacity: 0.6;
}

.viewBtn{
  position: absolute;
  bottom: 50rpx;
  right: 30rpx;
  display: flex;
  flex-flow: column;
  /* border: 1rpx solid red; */
}
 
.viewBtn text{
  font-size: 30rpx;
}

.icon-jiage{
  font-size: 20rpx;
  color: red;
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
}

.icon-jiage text:first-child{
    font-size: 27rpx;
}

.navigator{
  width: 400rpx;
  margin: 20rpx auto;
  height: 60rpx;
}

.navigator text{
  display: block;
  width: 60%;
  line-height: 60rpx;
  background-color: #ABCBF5;
  margin: auto auto;
  text-align: center;
  border-radius: 30rpx;
  color: white;
}


.startComputer{
  background-color: #E0FFFF;
  vertical-align: middle;
  padding: 50rpx;
}
.showTime{
  width: 80%;
  margin: 0 auto;
  position: relative;
}

.img{
  border-radius: 700rpx;
}

.time{
  top: 100rpx;
  position: absolute;
  left: 150rpx;
  font-size: 70rpx;
  /* width: 100%; */
  color: red;
  /* margin: 0 110rpx; */
}

.stopTime{
  margin: 100rpx auto;
}

.fixedrule{
  position: absolute;
  bottom: 50rpx;
  font-size: 35rpx;
  font-weight: bold;
  left: 250rpx;
  color: red;
  text-align: center;
}

.cost{
  font-size: 30rpx;
  margin: 0 auto;
  display: flex;
  flex-flow: column;
  background-color: white;
  z-index: 1;
  border-radius: 20rpx;
}

.cost .text{
  font-size: 38rpx;
  color: #ff9966;
  line-height: 50rpx;
  margin: 20rpx;
}

.action{
  display: none;
}

.icon-daohang1{
  float: right;
  margin-right: 30rpx;
  padding: 12rpx;
}

.checkMap::after{
  padding-left: 30rpx;
  content: '>';
}

.infoTitle::after{
  content: '>';
  padding: 10rpx 30rpx;
  font-size: 30rpx;
}

.clickAction{
  background-color: skyblue;
}

.footerBar{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  opacity: 0.5;
  background-color:#F3AB03;
}

.footerBar text{
  display: inline-block;
  width: 30%;
  text-align:center;
  height: 100%;
  line-height: 80rpx;
  margin: 0 70rpx;
}

.footerBar text:first-child{
  border-radius: 30rpx;
  
}

.footerBar text:last-child{
  border-radius: 30rpx;

}